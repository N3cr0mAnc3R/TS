
<link href="~/Content/Testing/Index.css" rel="stylesheet" />
<div id="main-window">
    @if (ViewBag.PlaceInfo != null)
    {
        <span>{{checkAuth(@ViewBag.PlaceInfo.PlaceId, @ViewBag.PlaceInfo.Id)}}</span>
    }
    else
    {
        <span>{{checkAuth(null,null)}}</span>
    }
    <div>
        @if (Request.IsAuthenticated)
        {

        <div v-if="!hasPlaceConfig" class="container">

            <div class="empty-list">
                {{switchLocal(4)}}
            </div>
            <div class="empty-list">
                {{switchLocal(12)}}
                <button v-on:click="askReset()" class="btn-reset">{{switchLocal(13)}}</button>
            </div>
        </div>
        }
        else
        {

            <div v-if="!hasPlaceConfig" class="container">
                <div class="main-window">
                    <div class="input-pin">
                        <input type="number" class="form-control" maxlength="4" v-model="PIN" placeholder="PIN">
                    </div>
                </div>
            </div>
        }
    <div v-if="hasPlaceConfig" class="test-info" v-loader="loadTestObject">
        <div class="not-test" v-if="verified">
            <div v-if="tests && tests.length == 0" class="waiting">
                <div class="empty-list">
                    {{switchLocal(11)}}
                </div>
            </div>
            <template v-if="activeTests.length == 0">
                <div v-if="tests && tests.length != 0" class="user-info">
                    <div class="user-name">{{user}},</div>
                    <div class="user-either">{{switchLocal(1)}}:</div>
                </div>
                <div class="test-list">
                    <div v-for="test in tests" class="test-wrapper">
                        @*- {{test.TestingDate}}*@
                        <div class="test-item" v-on:click="startTest(test.Id)">
                            <span class="test-name">{{test.DisciplineName}}</span>
                            <span class="test-icon">
                                <i class="far fa-play-circle"></i>
                            </span>
                        </div>
                    </div>
                </div>
            </template>
            <template v-if="activeTests.length > 0">
                <div v-if="tests && tests.length != 0" class="user-info">
                    <span class="user-name">{{user}}</span>,
                    <br />
                    <br />
                    {{switchLocal(2)}}:
                </div>
                <div class="test-list">
                    <div v-for="test in activeTests" class="test-wrapper">
                        @*- {{test.TestingDate}}*@
                        <div class="test-item" v-on:click="startTest(test.Id)">
                            <span class="test-name">{{test.DisciplineName}}</span>
                            <span class="test-icon">
                                <i class="far fa-play-circle"></i>
                            </span>
                        </div>
                    </div>
                </div>
            </template>
            <div class="not-test" v-loader="loadObject">
                <div class="user-info">
                    <div class="user-either">{{switchLocal(9)}}:</div>
                </div>
                <div class="test-list" v-if="activeTests.length == 0">
                    <div v-for="test in testingTests" class="test-wrapper">
                        @*- {{test.TestingDate}}*@
                        <div class="test-item" v-on:click="startTest(test.Id)">
                            <span class="test-name">{{test.DisciplineName}}</span>
                            <span class="test-icon">
                                <i class="far fa-play-circle"></i>
                            </span>
                        </div>
                    </div>
                </div>
            </div>

            @*<div class="finish-game"><a class="btn-gray" href="DownloadVideoFile?Id=67">Скачать</a></div>*@
            @*<div class="finish-game"><button class="btn-gray" v-on:click="download()">download</button></div>*@
        </div>

        <div class="disabled-info" v-if="!verified" v-loader="loadTestObject">
            {{switchLocal(6)}}
            <br />


            <div class="pre-video-wrapper">
                <div class="video">
                    <div class="camera-wrapper">
                        <video id="video1" autoplay muted playsinline poster="~/Content/novideo.png" v-bind:class="{'error': tryRec, 'no-face':noFace}">
                        </video>
                    </div>
                </div>
            </div>
            <button v-on:click="tryVerify" class="btn-verify" v-bind:disabled="!enabled" v-bind:class="{'disabled': !enabled}">{{switchLocal(7)}}</button>
            <div class="not-test" v-loader="loadObject">
                <div class="user-info">
                    <div class="user-either">{{switchLocal(9)}}:</div>
                </div>
                <div class="test-list" v-if="activeTests.length == 0">
                    <div v-for="test in testingTests" class="test-wrapper">
                        @*- {{test.TestingDate}}*@
                        <div class="test-item" v-on:click="startTest(test.Id)">
                            <span class="test-name">{{test.DisciplineName}}</span>
                            <span class="test-icon">
                                <i class="far fa-play-circle"></i>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="open-chat-wrapper">
            <div v-on:click="toggleChat()">
                <svg width="50" height="50" viewBox="0 0 76 74" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect width="76" height="74" fill="url(#pattern0)" />
                    <defs>
                        <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0" transform="translate(-0.000818434) scale(0.00116469)" />
                        </pattern>
                        <image id="image0" width="860" height="836" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
            </div>
            <div class="chat-unread-wrapper" v-if="unreadCount > 0">
                <span>
                    {{unreadCount}}
                </span>
            </div>
        </div>
        <div id="full-chat-wrapper" v-bind:class="{'opened': chat.IsOpened}">
            <div class="chat-panel">
                <span class="user-info">
                    {{switchLocal(5)}}
                </span>
                <div class="btn-list">
                    <span v-on:click="toggleChat()" class="close-btn">
                        <span>
                            <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <g clip-path="url(#clip0)">
                                    <path d="M7.66094 6.00005L11.2115 2.44952C11.6702 1.99078 11.6702 1.24748 11.2115 0.78874C10.7527 0.330003 10.0094 0.330003 9.55071 0.78874L6.00018 4.33967L2.44965 0.789139C1.99092 0.330402 1.24761 0.330402 0.788877 0.789139C0.33014 1.24788 0.33014 1.99118 0.788877 2.44992L4.33941 6.00045L0.788877 9.55098C0.33014 10.0097 0.33014 10.753 0.788877 11.2117C1.24761 11.6705 1.99092 11.6705 2.44965 11.2117L6.00018 7.66122L9.55071 11.2117C10.0095 11.6705 10.7528 11.6705 11.2115 11.2117C11.6702 10.753 11.6702 10.0097 11.2115 9.55098L7.66094 6.00005Z" fill="white" />
                                </g>
                                <defs>
                                    <clipPath id="clip0">
                                        <rect width="11.1111" height="11.1111" fill="white" transform="translate(0.444336 0.444443)" />
                                    </clipPath>
                                </defs>
                            </svg>

                        </span>
                    </span>
                </div>
            </div>
            <div class="text-chat-wrapper">
                <ul class="chat-wrapper">
                    <li v-for="message in chat.messages" class="chat-message-wrapper" v-bind:class="{'sender': isMe(message)}" v-bind:id="'message-' + message.Id">
                        <div class="chat-message"><div class="message-text">{{message.Message}}</div><div class="message-date">{{getDateFormat(message.Date)}}</div></div>
                        <br />
                    </li>
                </ul>
                <div class="send-message-wrapper">
                    <div class="message-input">
                        <textarea v-model="chat.Message" v-on:focus="subscribeEnter"></textarea>
                        <button v-on:click="sendMessage">
                            @*<span class="text">Отправить </span>*@
                            <span>
                                <img width="25" height="25" src="~/Content/send-chat.png" />
                            </span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
</div>

<script src="~/Content/aes.js"></script>
<script src="~/Content/Testing/pretest.js"></script>
